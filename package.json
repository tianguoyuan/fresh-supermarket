{
  "name": "fresh-supermarket",
  "type": "module",
  "version": "1.11.0",
  "private": true,
  "packageManager": "pnpm@9.12.1",
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "dev": "vite ",
    "build": "vite build",
    "build:uat": "vite build --mode uat",
    "build:prod": "vite build --mode prod",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx simple-git-hooks",
    "svgo": "svgo -f ./src/assets/icons -o ./src/assets/icons"
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@vant/area-data": "^1.5.2",
    "@vant/touch-emulator": "^1.4.0",
    "@vueuse/core": "^11.1.0",
    "@vueuse/integrations": "^12.0.0",
    "animate.css": "^4.1.1",
    "await-to-js": "^3.0.0",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "fuse.js": "^7.0.0",
    "html2canvas": "^1.4.1",
    "mathjs": "^13.2.1",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.4",
    "qrcode": "^1",
    "vant": "^4.9.7",
    "vconsole": "^3.15.1",
    "vue": "^3.5.11",
    "vue-i18n": "^10.0.4",
    "vue-router": "^4.4.5",
    "vue-waterfall-plugin-next": "^2.6.4",
    "vue3-count-to": "^1.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@iconify-json/carbon": "^1.2.2",
    "@intlify/unplugin-vue-i18n": "^5.2.0",
    "@types/node": "^22.7.5",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^0.63.4",
    "@unocss/eslint-plugin": "^0.63.4",
    "@unocss/preset-rem-to-px": "^0.63.4",
    "@unocss/reset": "^0.63.4",
    "@vant/auto-import-resolver": "^1.2.1",
    "@vitejs/plugin-legacy": "^6.0.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue-macros/volar": "^0.30.3",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.12.0",
    "eslint-plugin-format": "^0.1.2",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "pnpm": "^9.12.1",
    "postcss": "^8.4.47",
    "postcss-mobile-forever": "^4.1.6",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.79.5",
    "simple-git-hooks": "^2.11.1",
    "svgo": "^3.3.2",
    "taze": "^0.17.2",
    "typescript": "^5.5.4",
    "unocss": "^0.63.4",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-macros": "^2.12.3",
    "unplugin-vue-router": "^0.10.8",
    "unplugin-vue-setup-extend-plus": "^1.0.1",
    "vite": "^5.4.8",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-javascript-obfuscator": "^3.1.0",
    "vite-plugin-pwa": "^0.20.5",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vconsole": "^2.1.1",
    "vite-plugin-vue-devtools": "^7.4.6",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vitest": "^2.1.2",
    "vue-tsc": "^2.1.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --fix",
      "git add"
    ],
    "*.svg": [
      "svgo",
      "git add"
    ]
  }
}
